<table class="table">
 	<thead>
    	<tr>
      		<th>Name</th>
      		<th>Family</th>
      		<th>age</th>
    	</tr>
  	</thead>

  	<tbody>
    <% @user_profiles.each do |profile| %>
      
      <tr>
        <td><%= profile.name %></td>
        <td><%= profile.familyName %></td>
        <td><%= profile.age %></td>
        <td><%= link_to 'Show', profile_path(profile.id) , :class => "btn btn-primary"%></td>
      </tr>
    <% end %>
  	</tbody>
 </table>